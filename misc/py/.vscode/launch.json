        "version": "0.2.0",
        "configurations": [
                {
                        "name": "Python: Current File",
                        "type": "debugpy",
                        "request": "launch",
                        "program": "${file}",
                        "console": "integratedTerminal",
                        "justMyCode": true,
                        "env": {
                                // "GEVENT_SUPPORT": "True",
                                "DEBUG": "1",
                        },
                },
                {
                        "name": "Python: Current File with -d",
                        "type": "debugpy",
                        "request": "launch",
                        "program": "${file}",
                        "args": [
                                "-d"
                        ],
                        "console": "integratedTerminal",
                        "justMyCode": true,
                        "env": {
                                // "GEVENT_SUPPORT": "True"
                                "DEBUG": "1",
                        },
                }
        ]
}
