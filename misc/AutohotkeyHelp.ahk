#NoEnv
FileEncoding UTF-8

EnvGet LocalAppData, LOCALAPPDATA
EnvGet SystemDrive, SystemDrive
EnvGet SystemRoot, SystemRoot
laPrograms := LocalAppData "\Programs"

; 10.0.14393 is Windows 10 build 14393, also known as version 1607.
chmFailMinversion := "10.0.22000"
chmFailMaxversion := "10.0.22631" ; returned on all 23H2 versions
autohotkeyHelp := (VerCompare(A_OsVersion, ">=" chmFailMinversion) && VerCompare(A_OsVersion, "<" chmFailMaxversion))
                ? "https://www.autohotkey.com/docs/v1/"
                : FirstExisting( A_AhkDir "\AutoHotkey.chm"
                               , laPrograms "\AutoHotkey\AutoHotkey.chm"
                               , A_ProgramFiles "\AutoHotkey\AutoHotkey.chm" )
Run "%autohotkeyHelp%"

